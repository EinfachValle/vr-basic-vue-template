{
  "name": "vr-basic-vue-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vue/babel-preset-app": "^5.0.8",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.5",
    "bootstrap-vue": "^2.23.1",
    "browserslist": "^4.21.5",
    "core-js": "^3.30.2",
    "crypto-browserify": "^3.12.0",
    "dot-object": "^2.1.4",
    "file-uri-to-path": "^2.0.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.2",
    "jquery": "^3.7.0",
    "js-logger": "^1.6.1",
    "lint-staged": "^13.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "pinia": "^2.0.36",
    "validator": "^13.9.0",
    "vue": "^3.3.4",
    "vue-fuse": "^4.1.1",
    "vue-i18n": "^9.2.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-i18n-routing": "^0.13.0",
    "vue-loading-overlay": "^6.0.3",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.2.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.3.2",
    "babel-eslint": "^10.1.0",
    "css-loader": "^6.7.4",
    "cypress": "^12.12.0",
    "eslint": "^8.41.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.13.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jsdom": "^22.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "start-server-and-test": "^2.0.0",
    "style-loader": "^3.3.3",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-cli-plugin-i18n": "^2.3.2",
    "vue-loader": "^17.1.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.3"
  },
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "main": "babel.config.js",
  "keywords": ["template", "vue"],
  "author": "Valentin Roehle",
  "license": "ISC",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "actions/**/*.js",
      "initializers/**/*.js",
      "servers/**/*.js",
      "tasks/**/*.js",
      "src/**/*.{vue}",
      "!**/node_modules/**"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/actions/",
      "/initializers/",
      "/servers/",
      "/tasks/"
    ],
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "bugs": {
    "url": "https://github.com/EinfachValle/vr-basic-vue-template/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet --fix --ext .js,.vue src/",
      "git add"
    ],
    "**/*.{vue,html,js,json,scss,less,css,md}": [
      "prettier --config ./.prettierrc --ignore-path ./.eslintignore --write",
      "git add"
    ]
  }
}
